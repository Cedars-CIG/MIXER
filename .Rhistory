library(dplyr)
library(data.table)
library(ggplot2)
library(ggokabeito)
library(stringr)
library(patchwork)
show_range <- FALSE
process_data <- function(data) {
data |>
data.table::melt(
id.vars = c("SNP", "cluster_rank", "positive"),
measure.vars = c("p_AA", "p_Aa", "p_aa"),
variable.name = "allele",
value.name = "value"
) |>
(\(x) x[, .(
mean = mean(value),
median = median(value),
iqr_lo = quantile(value, 0.25),
iqr_hi = quantile(value, 0.75),
lo = min(value),
hi = max(value)
), c("allele", "cluster_rank", "positive")])() |>
dplyr::mutate(
allele = factor(data.table::fcase(
allele == "p_AA", 1,
allele == "p_Aa", 2,
allele == "p_aa", 3
), levels = c(1, 2, 3), labels = c("AA", "Aa", "aa")),
cluster = factor(cluster_rank, levels=(seq(1, max(cluster_rank)))),
positive = factor(positive, levels=c("Positive","Negative"))
)
}
## plot data
point_and_line_plot <- function(data, title = NULL, show_range = TRUE, legend_position = "none", y_lab = "predicted mean value") {
data |>
ggplot(aes(x = allele, y = median, group = cluster, color = cluster)) +
geom_line() +
{if (show_range) geom_segment(aes(y = lo, yend = hi, x = allele, xend = allele), linewidth = 0.75, alpha = 0.5)} +
geom_segment(aes(y = iqr_lo, yend = iqr_hi, x = allele, xend = allele), linewidth = 0.75, alpha = 0.5) + # add IQR
geom_point(size = 3) +
labs(
caption = title, # caption is below the plot
y = y_lab
) +
scale_color_okabe_ito() +
#facet_wrap(~positive+cluster, nrow=2, drop=FALSE) + # modify to add facet for positive/negative
facet_grid(positive ~ cluster , scales = "free_y", drop=FALSE,
labeller = labeller(positive = c("Positive" = "Positive Slope", "Negative" = "Negative Slope"))) +
theme_classic() +
theme(
plot.caption = element_text(hjust = 0.5, size = 12),
axis.title.x = element_blank(),
legend.position = legend_position,
panel.grid.major.y = element_line(color = "gray", size = 0.5)
)
}
